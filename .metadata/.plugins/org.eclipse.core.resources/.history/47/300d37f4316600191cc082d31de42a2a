package java0424_method;

public class Java055_method {

	public static void main(String[] args) {
		
	}//end main()
	public static int process(int x, int y, int z) {
		return x+y+z;
	}//end process()
	
	public static int plus(int[] arr) {
		int sum=0;
		for(int i=0; i<arr.length; i++) {
			sum=sum+arr[i];
		}
		return sum;
	}//end plus

}//end class
