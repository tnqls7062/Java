package java0426_class.part04;

public class Calc {
int first;
int second;
char ope; //배열에 저장된 연산자는 +,-,*,/

public Calc() {
	
}
public Calc(int first,int second, char ope) {
	this.first = first;
	this.second = second;
	this.ope=ope;
}
public int process() {
	int a =0;
	if (ope=='+') {
		a= first+second;
	}else if (ope=='-') {
		a= first-second;
	}else if (ope=='*') {
		a= first*second;
	}else if(ope=='/') {
		a=	first/second;
	}else 
		return 0;
	 
	
}//end process()

public void prn() {
	System.out.printf("%2d %c %2d = %d\n",first,ope,second,process());
}
}
