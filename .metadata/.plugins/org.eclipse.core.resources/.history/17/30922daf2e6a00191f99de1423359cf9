package java0429_static_access.part08;

public class Time {
	private int hour;
	private int minute;
	private int second;
	
	public Time() {
		
	}
	
	public void setHour(int hour) {
		this.hour=hour;
	}
	
	public int getHour() {
		return hour;
	}
	
	public void setMinute(int minute) {
		this.minute=minute;
	}
	
	public int getMinute() {
		return minute;
	}
	
	public void setSecond(int second) {
		this.second=second;
	}
	
	public int getSecond() {
		return second;
	}
}// end class
