package java0425_class;
/*
 * 사용자가 정의한 데이터 타입-사람객체
 */ class Person{
		String name;
		int age;
		char gen;
		
		public void eat() {
			System.out.println("먹는다");
		}
		public void run() {
			System.out.println("달린다");
		}
	}//end class
	public class Java065_class {
	
	public static void main(String[] args) {
		Person ps;
		ps= new Person();
		System.out.println(ps);

	}

}
