	public static void main(String[] args) {
		int data =10;
		//process()호출하면서 data변수의 값을 넘겨준 후
		//[출력결과]을 참조하여 구현하시오.
	}//end main()

	public static boolean process (int data) {
		//data 매개변수의 값이 짝수이면 true,
		//홀수이면 false를 리턴하는 프로그램을 구현하세요.
		return false;
		
		
	}//end process()
}//end class
