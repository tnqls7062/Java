package java0425_class.part05;

public class Java069_variable {

	public static void main(String[] args) {
MemberVar mv=new MemberVar();
System.out.print("Var_byte:%d\n",mv.var_byte);
System.out.print("Var_short:%d\n",mv.var_short);
System.out.print("Var_int:%d\n",mv.var_int);
System.out.print("Var_long:%d\n",mv.var_long);
System.out.print("Var_float:%.1f\n",mv.var_float);
System.out.print("Var_double:%.1f\n",mv.var_double);
System.out.print("Var_boolean:%b\n",mv.var_boolean);
System.out.print("Var_char:%c\n",mv.var_char);
System.out.print("int[]=%s\n",mv.num);
System.out.print("String=%d\n",mv.stn);

	}

}
