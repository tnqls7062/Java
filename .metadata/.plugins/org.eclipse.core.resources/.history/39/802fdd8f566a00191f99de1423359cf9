package java0430_inheritance.part02;

public class MyGrand{
	public MyGrand() { //7
		System.out.println("MyGrand"); //8
	}//9

	class MyFather extends MyGrand{
		public MyFather() { //5
			super();//6
			System.out.println("MyFather"); //10
		}
	}

	class MyChild extends MyFather{
		public MyChild() { //3
			super(); //4
			System.out.println("MyChild");
		}
	}
}
