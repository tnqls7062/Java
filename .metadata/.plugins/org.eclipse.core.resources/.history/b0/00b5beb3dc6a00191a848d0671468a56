package java0430_inheritance.part04;

public class Parent {
	String name;
	int age;

	public Parent() {

	}

	public Parent(String name, int age) {
		this.name = name;
		this.age = age;
	}
}// end class

class Son extends Parent {
	String dept;

	public Son() {

	}

	public Son(String name, int age, String dept) {
		super(name, age);
		this.dept = dept;
	}
	public void prn() {
		System.out.printf("%s %d %s\n", name,age,dept);
	}
}
