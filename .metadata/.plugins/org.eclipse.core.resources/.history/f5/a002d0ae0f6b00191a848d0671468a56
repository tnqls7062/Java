package java0430_inheritance.prob.part02;

public class StudentExam extends Human {
 private int id;

 
 StudentExam(String name, int age, int height, int weight, int id){
	 super(name, age,height,weight);
	 this.id=id;
 }

public int getId() {
	return id;
}

public void setId(int id) {
	this.id = id;
}
@Override
public String toString() {
	String data = name + "\t" + age + "\t" + height + "\t" + weight;
	return data;
}

}
